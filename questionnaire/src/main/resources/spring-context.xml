<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="csvDao" class="ru.otus.dao.CsvQuestionDao">
        <property name="fileName" value="questions.csv"/>

    </bean>
    <bean id="questionService" class="ru.otus.service.DefaultQuestionService">
        <constructor-arg name="dao" ref="csvDao"/>
    </bean>

    <bean id="questionnaireService" class="ru.otus.service.DefaultQuestionnaireService">
        <constructor-arg name="questionService" ref="questionService"/>
    </bean>

</beans>
